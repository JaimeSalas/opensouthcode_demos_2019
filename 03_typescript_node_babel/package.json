{
  "name": "typescript_node_babel",
  "version": "0.0.0",
  "description": "Package to demonstrate the use of babel with typescript and node",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./app/index.js",
    "dev": "nodemon --inspect --watch app app/index.js -e js,ts",
    "build": "cross-env BABEL_ENV=es babel ./app --out-dir ./build --extensions '.ts'"
  },
  "keywords": [
    "Node",
    "Express",
    "TypeScript",
    "Babel"
  ],
  "author": "Jaime Salas",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^5.2.0",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@types/express": "^4.16.1",
    "nodemon": "^1.19.0"
  }
}
